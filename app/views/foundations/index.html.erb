<p id="notice"><%= notice %></p>

<h1>Foundations</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foundations.each do |foundation| %>
      <tr>
        <td><%= foundation.title %></td>
        <td><%= foundation.body %></td>
        <td><%= foundation.count %></td>
        <td><%= link_to 'Show', foundation %></td>
        <td><%= link_to 'Edit', edit_foundation_path(foundation) %></td>
        <td><%= link_to 'Destroy', foundation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foundation', new_foundation_path %>
